<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane fx:id="anchorPane" prefHeight="800.0" prefWidth="1530.0" stylesheets="@../CSS/Game.css" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.civilization.FXMLcontrollers.DiplomacyPanelController">
    <children>
        <ImageView fitHeight="63.0" fitWidth="77.0" layoutX="69.0" layoutY="32.0" onMouseClicked="#backToGameMap" pickOnBounds="true" preserveRatio="true">
            <image>
                <Image url="@../PNG/images/back.png" />
            </image>
        </ImageView>
      <Button alignment="CENTER" layoutX="488.0" layoutY="47.0" mnemonicParsing="false" onAction="#goToTradePanel" prefHeight="26.0" prefWidth="176.0" style="-fx-background-radius: 100;" text="Trade Panel" textFill="RED">
         <font>
            <Font name="Copperplate" size="18.0" />
         </font>
      </Button>
      <Button alignment="CENTER" layoutX="832.0" layoutY="51.0" mnemonicParsing="false" onAction="#goToDemandPanel" prefHeight="26.0" prefWidth="176.0" style="-fx-background-radius: 100;" text="Demand Panel" textFill="RED">
         <font>
            <Font name="Copperplate" size="18.0" />
         </font>
      </Button>
    </children>
</AnchorPane>

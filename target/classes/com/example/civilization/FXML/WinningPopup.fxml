<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="500.0" prefWidth="500.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.civilization.FXMLcontrollers.WinningPopupController">
   <children>
      <ImageView fitHeight="800.0" fitWidth="1530.0" pickOnBounds="true">
         <image>
            <Image url="@../PNG/ARX/BackgroundARX.png" />
         </image>
      </ImageView>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="577.0" layoutY="109.0" prefHeight="44.0" prefWidth="375.0" text="You Won" textFill="WHITE">
         <font>
            <Font size="27.0" />
         </font>
      </Label>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="501.0" layoutY="233.0" prefHeight="44.0" prefWidth="522.0" text="What do you wish to do with the city?" textFill="WHITE">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Button fx:id="attachButton" layoutX="692.0" layoutY="338.0" mnemonicParsing="false" onMouseClicked="#attachCity" prefHeight="25.0" prefWidth="133.0" style="-fx-background-radius: 100;" text="Attach" textFill="#06521c">
         <font>
            <Font name="Copperplate" size="13.0" />
         </font></Button>
      <Button fx:id="DestroyButton" layoutX="694.0" layoutY="410.0" mnemonicParsing="false" onMouseClicked="#destroyCity" prefHeight="25.0" prefWidth="133.0" style="-fx-background-radius: 100;" text="Destroy" textFill="#750808">
         <font>
            <Font name="Copperplate" size="13.0" />
         </font></Button>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="663.0" layoutY="517.0" prefHeight="25.0" prefWidth="191.0" text="Label" textFill="#fffdfd">
         <font>
            <Font size="14.0" />
         </font>
      </Label>
   </children>
</AnchorPane>
